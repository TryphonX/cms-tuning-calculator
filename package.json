{
	"name": "cms-tuning-calculator",
	"version": "2.2.1",
	"private": true,
	"license": "GPL-3.0-or-later",
	"author": "Tryfon Xydas",
	"repository": {
		"type": "git",
		"url": "https://github.com/TryphonX/CMS-Tuning-Calculator.git"
	},
	"homepage": "https://TryphonX.github.io/CMS-Tuning-Calculator",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"predeploy": "npm run build",
		"prepare": "husky install",
		"deploy": "gh-pages -d build",
		"changelog": "auto-changelog --handlebars-setup ./changelog-config/setup.js && auto-changelog --handlebars-setup ./changelog-config/setup.js -o ./release-notes/RELEASE_NOTES.md -t ./changelog-config/release-notes.hbs",
		"push-tags": "git push --tags"
	},
	"dependencies": {
		"next": "^14.2.21",
		"react": "^18",
		"react-dom": "^18",
		"react-icons": "^5.0.1",
		"sharp": "^0.33.5"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.7.1",
		"@commitlint/config-conventional": "^17.7.0",
		"@react-docgen/cli": "^1.0.4",
		"@tailwindcss/postcss": "^4.1.11",
		"@types/node": "^20",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@typescript-eslint/eslint-plugin": "^7.6.0",
		"@typescript-eslint/parser": "^7.6.0",
		"auto-changelog": "^2.4.0",
		"daisyui": "^5.0.46",
		"eslint": "^8",
		"eslint-config-next": "14.1.4",
		"eslint-plugin-react": "^7.33.2",
		"gh-pages": "^6.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^16.1.2",
		"postcss": "^8",
		"tailwindcss": "^4.1.11",
		"typescript": "^5"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix"
		],
		"*.{ts,tsx}": [
			"tsc --noEmit"
		]
	}
}
